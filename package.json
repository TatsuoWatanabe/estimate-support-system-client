{
  "name": "estimate-support-system-client",
  "version": "0.1.0",
  "description": "estimate support system client.",
  "scripts": {
    "start": "concurrently \"npm run watch:tsc\" \"npm run watch:scss\" \"lite-server\" ",
    "webpack:dev": "npm run scss && webpack-dev-server --inline --progress",
    "webpack:build": "npm run scss && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "watch:tsc": "tsc -w",
    "tsc": "tsc",
    "watch:scss": "npm run scss && node-sass src/scss --output srcc/styles --source-map true -w",
    "scss": "node-sass src/scss --output srcc/styles --source-map true",
    "postinstall": "npm run scss && npm run tsc",
    "e2e": "concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run"
  },
  "keywords": [],
  "author": "",
  "repository": {},
  "license": "MIT",
